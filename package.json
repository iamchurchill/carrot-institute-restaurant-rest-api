{
  "name": "carrot",
  "version": "1.0.0",
  "description": "A restaurant RESTFul API for Carrot Institute",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint .",
    "jest:int": "jest --watch --coverage --verbose --bail tests",
    "start": "node index.js",
    "start:dev": "nodemon index.js",
    "db:init": "npx sequelize-cli init",
    "model:generate": "npx sequelize-cli model:generate --name Token --force --underscored --attributes user_id:uuid,expires:date",
    "seed:generate": "npx sequelize-cli seed:generate --name token",
    "db:up": "echo creating && npm run db:create && echo created && npm run db:migrate && echo migrated && echo seeding && npm run db:seed:all && echo seeded",
    "db:reset": "echo droping && npm run db:drop && echo dropped && npm run db:up",
    "db:create": "npx sequelize-cli db:create",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo",
    "db:seed:all": "npx sequelize-cli db:seed:all",
    "db:seed:undo": "npx sequelize-cli db:seed:undo --seed name-of-seed-as-in-data",
    "db:seed:undo:all": "npx sequelize-cli db:seed:undo:all",
    "db:rollback": "npx sequelize-cli db:migrate:undo",
    "db:rollback:all": "npx sequelize-cli db:migrate:undo:all",
    "db:drop": "[[ $NODE_ENV == production ]] && exit 1 || npx sequelize-cli db:drop",
    "format:check:all": "prettier --check .",
    "format:all": "npx prettier --write ."
  },
  "keywords": [
    "RESTful API",
    "Carrot",
    "Institute",
    "Restaurant"
  ],
  "author": "FRIEDRICH MEREDITH SAM",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.4.0",
    "cls-hooked": "^4.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "dotenv-expand": "^10.0.0",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.0",
    "module-alias": "^2.2.3",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "pg": "^8.11.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.32.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.3",
    "winston": "^3.9.0",
    "xml2js": "^0.6.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "babel-register": "^6.26.0",
    "eslint": "^8.43.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^16.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "prettier": "^2.8.8",
    "sequelize-cli": "^6.6.1",
    "supertest": "^6.3.3"
  },
  "_moduleAliases": {
    "@root": ".",
    "@config": "./config",
    "@controllers": "./controllers",
    "@classes": "./classes",
    "@helpers": "./helpers",
    "@middlewares": "./middlewares",
    "@models": "./models",
    "@routes": "./routes",
    "@validations": "./routes/validations"
  }
}
